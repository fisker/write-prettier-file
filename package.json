{
  "name": "write-prettier-file",
  "version": "2.2.0",
  "description": "write prettier formatted file",
  "homepage": "https://github.com/fisker/write-prettier-file#readme",
  "bugs": {
    "url": "https://github.com/fisker/write-prettier-file/issues"
  },
  "repository": "fisker/write-prettier-file",
  "funding": "https://github.com/fisker/write-prettier-file?sponsor=1",
  "license": "MIT",
  "author": {
    "name": "fisker Cheung",
    "email": "lionkay@gmail.com",
    "url": "https://www.fiskercheung.com/"
  },
  "sideEffects": false,
  "type": "module",
  "exports": "./index.js",
  "files": [
    "index.js",
    "lib"
  ],
  "scripts": {
    "build": "echo no need",
    "clean": "run-p clean:*",
    "clean:dist": "del-cli dist",
    "dist": "run-p dist:*",
    "dist:npm": "np --yolo --no-yarn",
    "format": "run-p format:*",
    "format:eslint": "yarn lint:eslint --fix",
    "format:markdown": "yarn lint:markdown --fix",
    "format:package-json": "sort-package-json \"package.json\" \"packages/*/package.json\"",
    "format:prettier": "yarn lint:prettier --write",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint \"**/*.{js,mjs,cjs,vue}\"",
    "lint:markdown": "markdownlint \"**/*.md\" --ignore \"**/node_modules/**\"",
    "lint:package-json": "yarn run format:package-json --check",
    "lint:prettier": "prettier \"**/*.{css,html,js,cjs,mjs,json,less,md,scss,ts,vue,yaml,yml}\" --check",
    "release": "run-s clean lint build test format dist",
    "test": "ava",
    "test-coverage": "nyc yarn test",
    "prepare": "husky install"
  },
  "ava": {
    "verbose": true
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ]
  },
  "dependencies": {
    "prettier-format": "3.1.0",
    "url-or-path": "2.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.1.1",
    "@fisker/eslint-config": "10.1.4",
    "@fisker/husky-config": "4.1.2",
    "@fisker/lint-staged-config": "3.1.5",
    "@fisker/prettier-config": "5.0.6",
    "ava": "4.2.0",
    "dedent": "0.7.0",
    "del-cli": "4.0.1",
    "eslint": "8.13.0",
    "husky": "7.0.4",
    "lint-staged": "12.3.8",
    "markdownlint-cli": "0.31.1",
    "npm-run-all": "4.1.5",
    "nyc": "15.1.0",
    "prettier": "2.6.2",
    "sort-package-json": "1.55.0",
    "tempy": "3.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
