{
  "name": "write-prettier-file",
  "version": "1.2.6",
  "description": "write prettier formatted file",
  "homepage": "https://github.com/fisker/write-prettier-file#readme",
  "bugs": {
    "url": "https://github.com/fisker/write-prettier-file/issues"
  },
  "repository": "fisker/write-prettier-file",
  "funding": "https://github.com/fisker/write-prettier-file?sponsor=1",
  "license": "MIT",
  "author": {
    "name": "fisker Cheung",
    "email": "lionkay@gmail.com",
    "url": "https://www.fiskercheung.com/"
  },
  "sideEffects": false,
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup --config",
    "clean": "run-p clean:*",
    "clean:dist": "del-cli dist",
    "dist": "run-p dist:*",
    "dist:npm": "np --yolo --no-yarn",
    "format": "run-p format:*",
    "format:eslint": "yarn lint:eslint --fix",
    "format:markdown": "yarn lint:markdown --fix",
    "format:package-json": "sort-package-json \"package.json\" \"packages/*/package.json\"",
    "format:prettier": "yarn lint:prettier --write",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint \"**/*.{js,mjs,cjs,vue}\"",
    "lint:markdown": "markdownlint \"**/*.md\" --ignore \"**/node_modules/**\"",
    "lint:package-json": "yarn run format:package-json --check",
    "lint:prettier": "prettier \"**/*.{css,html,js,cjs,mjs,json,less,md,scss,ts,vue,yaml,yml}\" --check",
    "release": "run-s clean lint build test format dist",
    "test": "ava",
    "test-coverage": "nyc yarn test"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "ava": {
    "require": [
      "esm"
    ],
    "verbose": true
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ]
  },
  "dependencies": {
    "prettier-format": "1.1.5"
  },
  "devDependencies": {
    "@babel/preset-env": "7.13.5",
    "@commitlint/cli": "12.0.1",
    "@fisker/commitlint-config": "1.3.1",
    "@fisker/eslint-config": "4.1.1",
    "@fisker/husky-config": "4.0.1",
    "@fisker/lint-staged-config": "3.0.1",
    "@fisker/prettier-config": "4.0.1",
    "ava": "3.15.0",
    "babel-plugin-transform-async-to-promises": "0.8.15",
    "cz-conventional-changelog": "3.3.0",
    "dedent": "0.7.0",
    "del-cli": "3.0.1",
    "eslint": "7.20.0",
    "esm": "3.2.25",
    "husky": "5.1.1",
    "lint-staged": "10.5.4",
    "markdownlint-cli": "0.26.0",
    "npm-run-all": "4.1.5",
    "nyc": "15.1.0",
    "prettier": "2.2.1",
    "rollup": "2.39.1",
    "rollup-plugin-babel": "4.4.0",
    "sort-package-json": "1.49.0",
    "tempy": "1.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "home": "https://github.com/fisker/write-prettier-file#readme"
}
